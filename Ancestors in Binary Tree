class Solution {
  public:
    // Function should return all the ancestor of the target node
       bool flag=false;
      void solve(Node*root,vector<int>&ans,int target){
          if(root==NULL){
              return;
          }
          if(root->data==target){
              flag=true;
              return ;
          }
          if(!flag)
          solve(root->left,ans,target);
          if(!flag)
          solve(root->right,ans,target);
          
          if(flag==true){
            ans.push_back(root->data);
          }
          
          return;
      }
    vector<int> Ancestors(struct Node *root, int target) {
        // Code here
       
         vector<int>ans;
          if(root->data==target){
            return ans;
        }
          solve(root,ans,target);
          
          return ans;
    }
};
